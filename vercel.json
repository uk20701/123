{
  "version": 2,
  "builds": [
    {
      "src": "api/expenses.js",
      "use": "@vercel/node"
    },
    {
      "src": "index.html",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "^/api/expenses$",
      "dest": "/api/expenses.js"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ]
}
